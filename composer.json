{
    "name": "novius/laravel-nova-visual-composer",
    "description": "A Laravel Nova field to compose blocks.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": ">=7.4",
        "intervention/image": "^2.4",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
      "friendsofphp/php-cs-fixer": "~2.15.0"
    },
    "autoload": {
        "psr-4": {
            "Novius\\NovaVisualComposer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Novius\\NovaVisualComposer\\NovaVisualComposerServiceProvider"
            ]
        }
    },
    "scripts": {
      "lint": [
        "php-cs-fixer fix --config .php_cs -vv --diff --allow-risky=yes --dry-run"
      ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
